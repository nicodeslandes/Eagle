<Application
    x:Class="Eagle.App"
    xmlns:vm="clr-namespace:Eagle.ViewModels"
    xmlns:cv="clr-namespace:Eagle.Common.Converters;assembly=Eagle.Common"
    xmlns:local="clr-namespace:Eagle"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Styles/Styles.Menu.xaml" />
      </ResourceDictionary.MergedDictionaries>
      <cv:VisibilityConverter x:Key="VisibilityConverter" />
      <local:MefBootstrapper x:Key="bootstrapper" />
    </ResourceDictionary>
  </Application.Resources>
</Application>